%link(rel="stylesheet" type="text/css" href="/style.css")
%h1 Welcome #{user_name}
%h4 Your Money: #{money} $
%a{:href => "/"} Home
%br/
%a{:href => "/show_my_items"} My Items
%br/
%a{:href => "/logout"} Logout
%h2 Items of #{user.name}
%ul
  - for item in items_of_user
    %li #{item.name}
    %ul Price = #{item.price} $
    %ul active = #{item.active}

%hr
Time: #{time.strftime("%H:%M:%S")}